{% extends "block--bare.html.twig" %}
{#
/**
 * @file
 * Theme override for tabs.
 *
 * @ingroup templates
 */
#}
{% block content %}
  {% if content %}
    <nav class="tabs container local-tasks-block" role="navigation" aria-label="{{ 'Tabs'|t }}">
      <div class="row">
        <div class="col-xs-12">
          {{ content }}
        </div>
      </div>
    </nav>
  {% endif %}
{% endblock %}
